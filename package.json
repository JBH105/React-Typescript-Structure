    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json}'",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "jest",
    "coverage": "npm test --coverage --watchAll --collectCoverage From='src/features/**/*.{ts,tsx}'"
