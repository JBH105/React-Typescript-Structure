  "lint-staged": {
    "!(path/to/excluded/dir/**/*)*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx,json}": "eslint --fix",
    "*.{ts,tsx,js,jsx,json,scss,css,md}": "prettier --write",
    "*.{js,scss,css,md}": "prettier --write",
    "*.js": "eslint  --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
